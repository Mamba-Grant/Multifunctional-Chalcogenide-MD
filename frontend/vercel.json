{
  "builds": [
    { "src": "api/main.py", "use": "@vercel/python" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".svelte-kit/output"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/main.py" },
    {
      "src": "/(.*)",
      "dest": ".svelte-kit/$1"
    }
  ]
}
